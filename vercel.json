{
  "outputDirectory": "public",
  "headers": [
    {
      "source": "/(.*)\\.(jpg|jpeg|gif|png|webp|svg|woff|woff2)",
      "headers": [
        { "key": "Cache-Control", "value": "max-age=31536000" }
      ]
    },
    {
      "source": "/(.*)\\.(js|css)",
      "headers": [
        { "key": "Cache-Control", "value": "max-age=604800" }
      ]
    }
  ],
  "cleanUrls": false
}
